<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../layouts/header-layout');%>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <%- include('../layouts/nav-mobile-layout');%>
        </header>
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">

            <%- include('../layouts/nav-sidebar-layout');%>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <%- include('../layouts/header-desktop-layout');%>
                </div>
            </header>
            <!-- END HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="container section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12 ">
                                <!-- USER DATA-->
                                <div class="user-data m-b-30">
                                    <h3 class="title-3 m-b-30">
                                        <i class="zmdi zmdi-account-calendar"></i>user data</h3>
                                    <div class="rs-select2--dark rs-select2--md m-r-10 m-b-30 rs-select2--border">
                                        <div class="rs-select2--dark rs-select2--md m-r-10 m-b-30 rs-select2--border">
                                            <select class="js-select2" id="select" name="property">
                                                    <option selected="selected" >Select users</option>
                                                    <option data-url="admin-user-data-view">Admin</option>
                                                    <option data-url="users-data-view">Users</option>
                                                    <option data-url="all-user-data-view">All</option>
                                                </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                        <div class="dropDownSelect2"></div>
                                    </div>
                                    <div class="table-responsive table--no-card m-b-40">
                                        <table class="table table-borderless table-striped table-earning " id="usersViewTable" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <td>No</td>
                                                    <td id="uid" style="display:none;">UID</td>
                                                    <td>Name</td>
                                                    <td>Email</td>
                                                    <td>Role</td>
                                                    <td>Address</td>
                                                    <td>Blood Group</td>
                                                    <td>Mobile No</td>
                                                    <td style="display:none;">Disabled</td>
                                                    <td>Type</td>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>

                                </div>
                                <!-- END USER DATA-->
                            </div>

                        </div>
                    </div>
                </div>


            </div>
        </div>

    </div>


    <%-include('../layouts/footer-layout');-%>

</body>

</html>